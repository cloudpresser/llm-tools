{
  "name": "azure-devops-pr-creator",
  "version": "1.0.0",
  "description": "A TypeScript library to create pull requests in Azure DevOps",
  "main": "dist/azureDevOpsClient.js",
  "types": "dist/azureDevOpsClient.d.ts",
  "type": "module",
  "bin": {
    "create-pr": "./bin/create-pr",
    "sop-generator": "./bin/sop-generator",
    "sop-splitter": "./bin/sop-splitter"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "start": "npx tsx cli.ts",
    "start-ai": "npx tsx cli.ts",
    "lint": "eslint .",
    "check-types": "tsc --noEmit"
  },
  "keywords": [
    "azure",
    "devops",
    "pull request",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@instructor-ai/instructor": "^0.0.5",
    "@tavily/core": "^0.0.2",
    "@types/table": "^6.3.2",
    "axios": "^0.27.2",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "columnify": "^1.6.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "inquirer": "8.2.4",
    "openai": "^4.0.0",
    "ora": "^8.1.0",
    "pretty-cli": "^0.0.14",
    "simple-git": "^3.27.0",
    "table": "^6.8.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/inquirer": "8.2.1",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.10.2",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^3.12.7",
    "typescript": "^5.7.2",
    "zod": "^3.22.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ]
  }
}
