{
  "name": "@cloudpresser/create-pr",
  "version": "1.6.0",
  "description": "CLI tool to create pull requests in Azure DevOps",
  "main": "dist/cli.js",
  "bin": {
    "create-pr": "dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "scripts": {
    "build": "esbuild src/cli.ts --bundle --platform=node --outdir=dist --format=esm --sourcemap --external:http --external:https --external:url --external:crypto --external:stream --external:zlib --external:buffer --external:util --external:net --external:tls --external:fs --external:path",
    "start": "npm run build && node dist/cli.js",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@cloudpresser/shared": "*",
    "axios": "^0.27.2",
    "chalk": "^5.3.0",
    "ora": "^8.1.0",
    "readline": "^1.3.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "esbuild": "^0.20.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.x.x"
  },
  "gitHead": "e4e72f4e5f307227a7bf836b83d5347c4141eda0"
}
